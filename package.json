{
	"name": "directus-extension-surveyjs",
	"description": "Bundles a module and api endpoints Directus extenstions with SurveyJS to make the form creation and submission process easier",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "module",
				"name": "creator",
				"source": "src/creator/index.ts"
			},
			{
				"type": "endpoint",
				"name": "survey-api",
				"source": "src/survey-api/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"dependencies": {
		"vue": "^3.4.37",
    "survey-core": "^1.11.11",
    "survey-vue3-ui": "^1.11.11",
    "survey-creator-core": "^1.11.11",
    "survey-creator-vue": "^1.11.11",
    "vue-router": "^4.4.3",
    "@vueuse/core": "^11.0.1",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.3.4",
    "@types/node": "^22.3.0",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0",
    "vue-i18n": "^9.4.0",
    "vue": "^3.4.37"
	}
}
