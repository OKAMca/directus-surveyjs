{
	"name": "directus-extension-surveyjs",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "module",
				"name": "creator",
				"source": "src/creator/index.ts"
			},
			{
				"type": "endpoint",
				"name": "survey-api",
				"source": "src/survey-api/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.0.1",
		"typescript": "^5.5.4",
		"vue": "^3.4.38",
		"@types/node": "^22.5.0"
	}
}
